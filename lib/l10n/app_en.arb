{
  "@@locale": "en",
  "appBarTitle": "Curling Scoreboard",
  "@appBarTitle": {
    "description": "The title shown in the top app bar"
  },
  "teamNameRed": "Red",
  "@teamNameRed": {
    "description": "The text used for a team using Red rocks."
  },
  "teamNameYellow": "Yellow ",
  "@teamNameYellow": {
    "description": "The text used for a team using Yellow rocks."
  },
  "buttonLabelYes": "Yes",
  "@buttonLabelYes": {
    "description": "Generic button text for a yes/affrim action."
  },
  "buttonLabelNo": "No",
  "@buttonLabelNo": {
    "description": "Generic button text for a no/disallow action."
  },
  "appBarAddScoreButtonLabel": "Add Score",
  "@buttonLabelAddScore": {
    "description": "Button text for adding a score to the game."
  },
  "appBarFinishGameButtonLabel": "Finish Game",
  "@buttonLabelFinishGame": {
    "description": "Button text for ending the game."
  },
  "gameStartDialogTitle": "Game Setup",
  "@gameStartDialogTitle": {
    "description": "Title of the dialog shown to setup the game."
  },
  "gameStartDialogFormLabelNumberOfEnds": "Number of Ends:",
  "@gameStartDialogFormLabelNumberOfEnds": {
    "description": "Description text for the number of ends setting in the game start dialog."
  },
  "gameStartDialogFormLabelPlayersPerTeam": "Players per Team:",
  "@gameStartDialogFormLabelPlayersPerTeam": {
    "description": "Description text for the number of players setting in the game start dialog."
  },
  "gameStartDialogTimePerEndByPlayersButtonLabel": "{minutesPerEnd} minutes per end, {gameTotalTime} total",
  "@gameStartDialogTimePerEndByPlayersButtonLabel": {
    "description": "Button label for how long each end is when setting up a game.",
    "placeholders": {
      "minutesPerEnd": {
        "type": "String",
        "example": "15"
      },
      "gameTotalTime": {
        "type": "String",
        "example": "2h"
      }
    }
  },
  "gameStartDialogButtonLabelStartGame": "Start Game",
  "@gameStartDialogButtonLabelStartGame": {
    "description": "Button label to start the game."
  },
  "finishGameConfirmationDialogDescription": "Are you sure you want to finish the game?",
  "@finishGameDialogDescription": {
    "description": "Description for the dialog that shows up when a user attempts to finish the game."
  },
  "gameEndDialogTitle": "Game Report",
  "@gameEndDialogTitle": {
    "description": "Title of the dialog shown at the end of a game."
  },
  "gameEndDialogButtonLabelDismiss": "Dismiss",
  "@gameEndDialogButtonLabelDismiss": {
    "description": "Button label to dismiss the end game dialog"
  },

  "settingsLabelVersion": "Version {versionNumber} ",
  "@settingsLabelVersion": {
    "description": "Version label in the settings dialog.",
    "placeholders": {
      "versionNumber": {
        "type": "String",
        "example": "1.0.0"
      }
    }
  },
  "scoreInputDialogTitle": "Score - End {endNumberName}",
  "@scoreInputDialogTitle": {
    "description": "Title text for the edit score dialog.",
    "placeholders": {
      "endNumberName": {
        "type": "String",
        "example": "1"
      }
    }
  },
  "scoreInputEnterButtonLabel": "Enter",
  "@scoreInputEnterButtonLabel": {
    "description": "Button text for score input dialog to submit the score."
  },
  "addScoreGameCompleteMessage": "Game is complete. Finish the game to reset.",
  "@addScoreGameCompleteMessage": {
    "description": "Message shown to the user when they try and enter a score for a game that has been completed."
  },
  "gameInfoGameTimeLabel": "Game Time {gameTimeString}",
  "@gameInfoGameTimeLabel": {
    "description": "Text for the game info section to show the current game time",
    "placeholders": {
      "gameTimeString": {
        "type": "String",
        "example": "00:00:00"
      }
    }
  },
  "scoreboardExtraEndLabel": "E",
  "@scoreboardExtraEndLabel": {
    "description": "Label for the scoreboard for the extra end"
  }
}